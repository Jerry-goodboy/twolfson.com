<section id="portfolioSection">
  <article>
    <style>
      .tempA {
        overflow: visible;
        white-space: nowrap;
        position:relative;
      }

      .tempB {
        position: absolute;
        right: 0;
      }

      .tempC,
      .tempD,
      .tempE,
      .tempF,
      .tempG,
      .tempH {
        background-image: url('public/images/sprites.png');
        background-repeat: no-repeat;
        font-size: 20px;
        padding-right: 20px;
        background-size: 20px auto;
        vertical-align: middle;
      }

      .tempC {
        background-image: url('public/images/purpleStar.png');
        margin-top: -5px;
        margin-right: 5px;
      }

      .tempD {
        background-image: url('public/images/purpleFork.png');
        margin-top: -1px;
        margin-right: 5px;
      }

      .tempE {
        background-image: url('public/images/barLines.png');
        margin-top: -5px;
        margin-right: 15px;
      }

      .tempF {
        background-image: url('public/images/eye.png');
        /*height: 14px;*/
        /*background-size: 14px;*/
        vertical-align: middle;
        margin-top: -3px;
        margin-right: 10px;
      }

      .tempG {
        background-image: url('public/images/heart.png');
        /*height: 18px;*/
        /*background-size: 18px;*/
        vertical-align: middle;
        margin-top: -4px;
        margin-right: 10px;
      }

      .tempH {
        background-image: url('public/images/ribbon.png');
        /*height: 30px;*/
        /*background-size: 30px;*/
        vertical-align: middle;
        margin-top: -5px;
        margin-right: 7px;
      }
    </style>
    <div>
      <%
        var _rowCounter = 1;
        function getRowClass() {
          var klass = _rowCounter % 2 ? "themeOddRow" : "themeEvenRow";
          _rowCounter += 1;
          return klass;
        }
      %>
      <div>
        <h2 class="headerMega layoutCentered superComfyText <%=getRowClass()%>">Scripts</h2>
      </div>
      <%
        scripts.forEach(function (item) {
      %>
      <div class="l_row superComfyText <%=getRowClass()%>">
        <%- partial('projects/script', item) %>
      </div>
      <%
        });
      %>
    </div>
    <div>
      <div>
        <h2 class="headerMega layoutCentered superComfyText superComfyRowTop <%=getRowClass()%>">Competitions</h2>
      </div>
      <%
        var lenMinusOne = competitions.length - 1;
        competitions.forEach(function (item, i) {
          var isEven = i % 2 === 0,
              isLastItem = i === lenMinusOne,
              isOwnRow = isEven && isLastItem,
              isRowStart = isEven,
              isRowEnd = !isEven || isOwnRow,
              comfyRowKlass = isLastItem ? "" : "comfyRow",
              view = isOwnRow ? 'projects/competitionThin' : 'projects/competition';
      %>
        <% if (isRowStart) { %>
          <div class="l_row <%=comfyRowKlass%> <%=getRowClass()%>">
        <% } %>
            <%- partial(view, item) %>
        <% if (isRowEnd) { %>
          </div>
        <% } %>
      <%
        });
      %>
    </div>
    <div>
      <div>
        <h2 class="headerMega layoutCentered superComfyText superComfyRowTop <%=getRowClass()%>">Contributions</h2>
      </div>
      <%
        contributions.forEach(function (item) {
      %>
      <div class="l_row superComfyText <%=getRowClass()%>">
        <%- partial('projects/script', item) %>
      </div>
      <%
        });
      %>
    </div>
  </article>
</section>