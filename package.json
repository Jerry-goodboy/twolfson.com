{
  "name": "twolfson.com",
  "description": "twolfson.com server",
  "version": "3.69.3",
  "homepage": "https://github.com/twolfson/twolfson.com",
  "author": {
    "name": "Todd Wolfson",
    "email": "todd@twolfson.com",
    "url": "http://twolfson.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twolfson/twolfson.com.git"
  },
  "bugs": {
    "url": "https://github.com/twolfson/twolfson.com/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/twolfson/twolfson.com/blob/master/LICENSE-MIT"
    }
  ],
  "bin": {
    "twolfson.com": "bin/twolfson.com"
  },
  "main": "server/index.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "accept-pdiffs": "cp test/perceptual-tests/actual_screenshots/* test/perceptual-tests/expected_screenshots/",
    "lint": "jshint bin/ config/ docs/ grunt/ server/ test/integration-tests/ test/perceptual-tests/ test/production-tests/ test/utils/ *.js && grunt jshint:client && jscs bin/ config/ docs/ grunt/ public/js/main.js server/  test/integration-tests/ test/perceptual-tests/ test/production-tests/ test/utils/ *.js",
    "pretest": "twolfson-style install",
    "start": "bin/twolfson.com",
    "test": "npm run lint && npm run test-integration",
    "test-travis": "npm run lint && npm run test-integration",
    "test-perceptual": "node test/perceptual-tests/twolfson.com_test.js",
    "test-integration": "mocha test/integration-tests",
    "test-production": "mocha test/production-tests",
    "update-projects": "node bin/update-projects.js",
    "vagrant-test-perceptual": "vagrant ssh -c \"cd /vagrant && npm run test-perceptual\""
  },
  "dependencies": {
    "async": "~0.2.9",
    "backbone": "~1.1.2",
    "commander": "~2.1.0",
    "emailjs": "0.3.4",
    "express": "~3.4.8",
    "express-partials": "~0.1.1",
    "github": "~0.2.2",
    "jade": "~1.9.2",
    "jojo": "~3.0.1",
    "marked": "~0.2.6",
    "numscale": "0.0.3",
    "request": "~2.27.0",
    "rollbar": "~0.3.1",
    "shallow-settings": "~0.1.0",
    "underscore": "~1.6.0",
    "unhtml": "~0.1.0"
  },
  "devDependencies": {
    "chai": "~1.9.0",
    "css-controls": "~0.1.1",
    "foundry": "~4.3.2",
    "foundry-release-git": "~2.0.2",
    "foundry-release-npm": "~2.0.2",
    "grunt": "~0.4.5",
    "grunt-cli": "~0.1.13",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-jshint": "~0.6.4",
    "grunt-contrib-sass": "~0.5.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-curl": "~1.2.1",
    "grunt-image-resize": "~0.3.0",
    "grunt-jsbeautifier": "~0.2.3",
    "grunt-jsmin-sourcemap": "~1.8.0",
    "grunt-spritesmith": "~4.5.0",
    "grunt-zip": "~0.9.2",
    "image-diff": "~1.0.1",
    "js-yaml": "~2.1.0",
    "jscs": "~1.7.3",
    "jsdom": "~0.5.1",
    "jshint": "~2.5.10",
    "mocha": "~1.8.1",
    "request-mocha": "~0.2.0",
    "rimraf": "~2.2.6",
    "shell-quote": "~1.4.1",
    "simplesmtp": "~0.3.21",
    "slug": "~0.3.3",
    "temporary": "0.0.5",
    "twolfson-style": "~1.3.0",
    "xml2js": "~0.4.1"
  },
  "foundry": {
    "releaseCommands": [
      "foundry-release-git",
      "foundry-release-npm"
    ]
  },
  "private": true
}