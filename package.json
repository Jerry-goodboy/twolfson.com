{
  "name": "twolfson.com",
  "description": "twolfson.com server",
  "version": "3.91.0",
  "homepage": "https://github.com/twolfson/twolfson.com",
  "author": {
    "name": "Todd Wolfson",
    "email": "todd@twolfson.com",
    "url": "http://twolfson.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twolfson/twolfson.com.git"
  },
  "bugs": {
    "url": "https://github.com/twolfson/twolfson.com/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/twolfson/twolfson.com/blob/master/LICENSE-MIT"
    }
  ],
  "bin": {
    "twolfson.com": "bin/twolfson.com"
  },
  "main": "server/index.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "accept-pdiffs": "cp test/perceptual-tests/actual_screenshots/* test/perceptual-tests/expected_screenshots/",
    "build": "gulp build-clean && gulp build",
    "develop": "gulp build-clean && gulp develop",
    "lint": "jshint bin/ config/ docs/ server/ test/integration-tests/ test/perceptual-tests/ test/production-tests/ test/utils/ *.js && jscs bin/ config/ docs/ public/js/main.js server/ test/integration-tests/ test/perceptual-tests/ test/production-tests/ test/utils/ *.js",
    "pretest": "twolfson-style install",
    "postinstall": "bin/decrypt-config.sh",
    "start": "bin/twolfson.com",
    "test": "npm run lint && npm run test-integration",
    "test-travis": "npm run lint && npm run test-integration",
    "test-perceptual": "node test/perceptual-tests/twolfson.com_test.js",
    "test-integration": "mocha test/integration-tests",
    "test-production": "mocha test/production-tests",
    "update-projects": "node bin/update-projects.js",
    "vagrant-test-perceptual": "vagrant ssh -c \"cd /vagrant && npm run test-perceptual\""
  },
  "dependencies": {
    "async": "~0.2.9",
    "backbone": "~1.1.2",
    "commander": "~2.1.0",
    "emailjs": "0.3.4",
    "express": "~3.4.8",
    "express-partials": "~0.1.1",
    "github": "~0.2.2",
    "jade": "~1.9.2",
    "jojo": "~3.0.1",
    "marked": "~0.2.6",
    "numscale": "0.0.3",
    "request": "~2.27.0",
    "rollbar": "~0.3.1",
    "shallow-settings": "~0.1.0",
    "underscore": "~1.6.0",
    "unhtml": "~0.1.0"
  },
  "devDependencies": {
    "chai": "~1.9.0",
    "css-controls": "~0.1.1",
    "foundry": "~4.3.2",
    "foundry-release-git": "~2.0.2",
    "foundry-release-npm": "~2.0.2",
    "gulp": "~3.9.1",
    "gulp-concat": "~2.6.0",
    "gulp-csso": "~1.0.1",
    "gulp-livereload": "~3.8.1",
    "gulp-sass": "~2.2.0",
    "gulp-sizereport": "~1.1.3",
    "gulp-uglify": "~1.5.2",
    "gulp.spritesmith": "~6.2.0",
    "image-diff": "~1.0.1",
    "js-yaml": "~2.1.0",
    "jscs": "~1.7.3",
    "jsdom": "~0.5.1",
    "jshint": "~2.5.10",
    "merge-stream": "~1.0.0",
    "mocha": "~1.8.1",
    "request-mocha": "~0.2.0",
    "rimraf": "~2.2.6",
    "shell-quote": "~1.4.1",
    "simplesmtp": "~0.3.21",
    "slug": "~0.3.3",
    "temporary": "0.0.5",
    "twolfson-style": "~1.3.0",
    "xml2js": "~0.4.1"
  },
  "foundry": {
    "releaseCommands": [
      "foundry-release-git",
      "foundry-release-npm"
    ]
  },
  "private": true
}